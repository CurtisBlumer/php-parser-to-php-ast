    "require": {
        "php": ">=7.1",
        "nikic/PHP-Parser": "3.0.4",
        "ext-ast": "~0.1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {"ASTConverter\\": "src/ASTConverter"}
